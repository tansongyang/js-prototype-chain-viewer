  "bugs": {
    "url": "https://github.com/tansongyang/js-prototype-chain-viewer/issues"
  },
  "homepage": "https://github.com/tansongyang/js-prototype-chain-viewer",
  "devDependencies": {
    "babel": "^5.8.19",
    "babel-core": "^5.8.19",
    "babelify": "^6.1.3",
    "browserify": "^10.2.6",
    "chai": "^3.2.0",
    "jshint": "^2.8.0",
    "mocha": "^2.2.5",
    "nodemon": "^1.4.1",
    "parallelshell": "^2.0.0",
    "watch": "^0.16.0"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }

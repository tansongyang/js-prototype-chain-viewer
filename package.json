{
  "name": "js-prototype-chain-viewer",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "build": "npm run build:debug",
    "build:debug": "npm run build:scripts -- -d",
    "build:scripts": "browserify -e scripts/index.js -o scripts/bundle.js",
    "clean": "rm -f scripts/bundle.js",
    "rebuild": "npm run clean && npm run build",
    "test": "mocha --require test/helper/config.js --compilers js:babel/register",
    "watch": "parallelshell \"npm run watch:scripts\" \"npm run watch:test\"",
    "watch:scripts": "nodemon -e js -w scripts/ \"npm run build-debug\"",
    "watch:test": "npm run test -- -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tansongyang/js-prototype-chain-viewer.git"
  },
  "author": "Frank Tan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tansongyang/js-prototype-chain-viewer/issues"
  },
  "homepage": "https://github.com/tansongyang/js-prototype-chain-viewer",
  "devDependencies": {
    "babel": "^5.8.19",
    "babel-core": "^5.8.19",
    "babelify": "^6.1.3",
    "browserify": "^10.2.6",
    "chai": "^3.2.0",
    "jshint": "^2.8.0",
    "mocha": "^2.2.5",
    "nodemon": "^1.4.1",
    "parallelshell": "^2.0.0",
    "watch": "^0.16.0"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
